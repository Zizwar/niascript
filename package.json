{
 "name": "niascript",
 "version": "1.3.1",
 "description": "Intent-based programming language for the AI era",
 "bin": {
  "nia": "./bin/nia-cli.js"
 },
 "type": "module",
 "scripts": {
  "start": "node examples/enhanced-demo.js",
  "start:env": "node -r dotenv/config examples/enhanced-demo.js",
  "cli": "node bin/nia-cli.js",
  "cli:env": "node -r dotenv/config bin/nia-cli.js",
  "test": "node bin/nia-cli.js test",
  "test:env": "node -r dotenv/config bin/nia-cli.js test",
  "interactive": "node bin/nia-cli.js interactive",
  "interactive:env": "node -r dotenv/config bin/nia-cli.js interactive",
  "dev": "nodemon examples/enhanced-demo.js",
  "dev:env": "nodemon -r dotenv/config examples/enhanced-demo.js",
  "email": "nodemon examples/email-demo.js",
  "email:env": "nodemon -r dotenv/config examples/email-demo.js",
  "build": "rollup -c",
  "docs": "jsdoc -d docs src/",
  "stats": "node bin/nia-cli.js stats",
  "stats:env": "node -r dotenv/config bin/nia-cli.js stats",
  "install-global": "npm install -g ."
 },
 "dependencies": {
  "axios": "^1.6.0",
  "chalk": "^5.3.0",
  "commander": "^11.0.0",
  "dotenv": "^17.0.1",
  "inquirer": "^9.2.0",
  "openai": "^4.0.0"
 },
 "devDependencies": {
  "jest": "^29.0.0",
  "jsdoc": "^4.0.4",
  "nodemon": "^3.0.0",
  "rollup": "^4.0.0"
 },
 "keywords": [
  "ai",
  "nlp",
  "intent",
  "programming",
  "plugins",
  "automation",
  "clean-architecture"
 ],
 "author": "Brahim BIDI",
 "license": "MIT",
 "engines": {
  "node": ">=16.0.0"
 }
}
